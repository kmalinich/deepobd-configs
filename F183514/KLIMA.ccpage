				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_BELUEFTUNG_WERT" result="STAT_BELUEFTUNG_WERT" log_tag="STAT_BELUEFTUNG_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_MISCHLUFT_LINKS_WERT" result="STAT_MISCHLUFT_LINKS_WERT" log_tag="STAT_MISCHLUFT_LINKS_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FOND_WERT" result="STAT_FOND_WERT" log_tag="STAT_FOND_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRONT_SCHICHTUNG_WERT" result="STAT_FRONT_SCHICHTUNG_WERT" log_tag="STAT_FRONT_SCHICHTUNG_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_PTC_IST_WERT" result="STAT_PTC_IST_WERT" log_tag="STAT_PTC_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_DEFROST_IST_WERT" result="STAT_DEFROST_IST_WERT" log_tag="STAT_DEFROST_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRISCHLUFT_UMLUFT_IST_WERT" result="STAT_FRISCHLUFT_UMLUFT_IST_WERT" log_tag="STAT_FRISCHLUFT_UMLUFT_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_MISCHLUFT_RECHTS_IST_WERT" result="STAT_MISCHLUFT_RECHTS_IST_WERT" log_tag="STAT_MISCHLUFT_RECHTS_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FUSSRAUM_IST_WERT" result="STAT_FUSSRAUM_IST_WERT" log_tag="STAT_FUSSRAUM_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_BELUEFTUNG_IST_WERT" result="STAT_BELUEFTUNG_IST_WERT" log_tag="STAT_BELUEFTUNG_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_MISCHLUFT_LINKS_IST_WERT" result="STAT_MISCHLUFT_LINKS_IST_WERT" log_tag="STAT_MISCHLUFT_LINKS_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FOND_IST_WERT" result="STAT_FOND_IST_WERT" log_tag="STAT_FOND_IST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRONT_SCHICHTUNG_IST_WERT" result="STAT_FRONT_SCHICHTUNG_IST_WERT" log_tag="STAT_FRONT_SCHICHTUNG_IST_WERT" />
			</job>
			<job name="STATUS_UIF_SENSOR">
				<display name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_WERT" result="STAT_TEMP_INNEN_WERT" format="L" log_tag="STAT_TEMP_INNEN_WERT" />
				<display name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_NTC1_WERT" result="STAT_TEMP_INNEN_NTC1_WERT" format="L" log_tag="STAT_TEMP_INNEN_NTC1_WERT" />
				<display name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_NTC2_WERT" result="STAT_TEMP_INNEN_NTC2_WERT" format="L" log_tag="STAT_TEMP_INNEN_NTC2_WERT" />
				<display name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_SOLAR_WERT" result="STAT_TEMP_INNEN_SOLAR_WERT" format="L" log_tag="STAT_TEMP_INNEN_SOLAR_WERT" />
			</job>
		</jobs>
	</page>
</fragment>
