<?xml version="1.0" encoding="utf-8"?>
<fragment xmlns="http://www.holeschak.de/BmwDeepObd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.holeschak.de/BmwDeepObd ../BmwDeepObd.xsd">
	<page name="!PAGE_NAME" display-mode="list" fontsize="medium" gauges-portrait="3" gauges-landscape="6" logfile="KLIMA.{D}.log">
		<strings>
			<string name="!PAGE_NAME">Climate</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_DISPLAY_WERT">Display temp</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_ML_RECHTS_WERT">Mixing flap right</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_ML_LINKS_WERT">Mixing flap left</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_FUSSR_RECHTS_WERT">Footwell temp right</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_FUSSR_LINKS_WERT">Footwell temp left</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_VERDAMPFER_WERT">Evaporator temp</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_INNEN_WERT">Interior temp</string>
			<string name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_WERT">Interior temp (calc)</string>
			<string name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_NTC1_WERT">Interior temp NTC1</string>
			<string name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_NTC2_WERT">Interior temp NTC2</string>
			<string name="!JOB#STATUS_UIF_SENSOR#STAT_TEMP_INNEN_SOLAR_WERT">Interior solar sensor</string>
			<string name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_SOLARSENSOR_WERT">Solar sensor</string>

			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_DEFROST_WERT">Defrost</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRISCHLUFT_UMLUFT_WERT">Fresh air reverse</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRONT_SCHICHTUNG_WERT">Front stratification</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FUSSRAUM_WERT">Footwell</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_MISCHLUFT_LINKS_WERT">Mixed air left</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_MISCHLUFT_RECHTS_WERT">Mixed air right</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_PTC_WERT">PTC</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FOND_WERT">Rear</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_BELUEFTUNG_WERT">Ventilation</string>

			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_DEFROST_IST_WERT">Defrost (target)</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRISCHLUFT_UMLUFT_IST_WERT">Fresh air reverse (target)</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRONT_SCHICHTUNG_IST_WERT">Front stratification (target)</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FUSSRAUM_IST_WERT">Footwell (target)</string>
