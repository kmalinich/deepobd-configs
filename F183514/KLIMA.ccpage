			</job>
			<job name="STATUS_MOTOR_KLAPPENPOSITION">
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_PTC_WERT" result="STAT_PTC_WERT" log_tag="STAT_PTC_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_DEFROST_WERT" result="STAT_DEFROST_WERT" log_tag="STAT_DEFROST_WERT" />
				<display name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FRISCHLUFT_UMLUFT_WERT" result="STAT_FRISCHLUFT_UMLUFT_WERT" log_tag="STAT_FRISCHLUFT_UMLUFT_WERT" />
