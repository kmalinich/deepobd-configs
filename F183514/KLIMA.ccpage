			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_PTC_IST_WERT">PTC (target)</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_FOND_IST_WERT">Rear (target)</string>
			<string name="!JOB#STATUS_MOTOR_KLAPPENPOSITION#STAT_BELUEFTUNG_IST_WERT">Ventilation (target)</string>
		</strings>
		<jobs sgbd="IHKA87_2">
			<job name="STATUS_SENSOREN_ANALOG_PORT">
				<display name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_ML_RECHTS_WERT" result="STAT_TEMP_ML_RECHTS_WERT" format="L" log_tag="STAT_TEMP_ML_RECHTS_WERT" />
				<display name="!JOB#STATUS_SENSOREN_ANALOG_PORT#STAT_TEMP_ML_LINKS_WERT" result="STAT_TEMP_ML_LINKS_WERT" format="L" log_tag="STAT_TEMP_ML_LINKS_WERT" />
